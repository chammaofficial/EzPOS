                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT Products.Name, Products.Description, Products.ModelNo, Branches.Code, Branches.Name AS BranchName, PurchaseOrderDetails.OrderQty, PurchaseOrderDetails.LineItemNo, PurchaseOrders.PoDate, PurchaseOrders.DeliveryDate, 
                  PurchaseOrders.Remark, Suppliers.Name AS SupplierName, Suppliers.Address, Suppliers.Phone, PurchaseOrders.Id
FROM     Branches INNER JOIN
                  Products ON Branches.Id = Products.Id INNER JOIN
                  PurchaseOrderDetails ON Products.Id = PurchaseOrderDetails.ProductId INNER JOIN
                  PurchaseOrders ON Branches.Id = PurchaseOrders.BranchId AND PurchaseOrderDetails.PurchaseOrderId = PurchaseOrders.Id INNER JOIN
                  Suppliers ON PurchaseOrders.SupplierId = Suppliers.Id
WHERE  (PurchaseOrders.Id = @POID)</CommandText>
